<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xd.mapper.FunctionMapper">

    <resultMap id="BaseResultMap" type="com.xd.entity.Functions">
            <id property="functionId" column="function_id" jdbcType="INTEGER"/>
            <result property="functionCode" column="function_code" jdbcType="VARCHAR"/>
            <result property="functionName" column="function_name" jdbcType="VARCHAR"/>
            <result property="functionIcon" column="function_icon" jdbcType="VARCHAR"/>
            <result property="functionLevel" column="function_level" jdbcType="INTEGER"/>
            <result property="functionFather" column="function_father" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        function_id,function_code,function_name,
        function_icon,function_level,function_father
    </sql>
</mapper>
